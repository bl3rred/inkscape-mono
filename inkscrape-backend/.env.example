# Server
PORT=3001
NODE_ENV=production

# Database
MONGODB_URI=mongodb+srv://<db_user>:<db_password>@<cluster>/<db_name>?retryWrites=true&w=majority

# Auth0
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_AUDIENCE=https://api.inkscape

# Development auth bypass (set false in production)
DEV_AUTH=false

# Optional CORS allow-list (comma-separated exact origins)
# Example: CORS_ALLOWED_ORIGINS=https://app.example.com,https://staging.example.com
CORS_ALLOWED_ORIGINS=

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
