{
  "installCommand": "npm install --include=optional",
  "buildCommand": "node -e \"const cp=require('node:child_process');const path=require('node:path');const viteBin=path.join(path.dirname(require.resolve('vite/package.json')),'bin','vite.js');const r=cp.spawnSync(process.execPath,[viteBin,'build'],{stdio:'inherit'});process.exit(r.status ?? 1)\"",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
